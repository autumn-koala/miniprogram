/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.decoration{
  position: fixed;
  top: 100vh;
  left: 0;
  z-index: 9999;
  width: 100%;
  height: 1rpx;
  box-shadow: 0 0 20rpx rgba(0,0,0,.4);
}