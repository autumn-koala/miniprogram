/* pages/cart/cart.wxss */



.address_bar {
  height: 90rpx;
  padding: 0 20rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1rpx solid #e6e6e6;
}

.address_bar .left {
  width: 216rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 28rpx;
  line-height: 90rpx;
  color: #999;
  padding-left: 36rpx;
  background: url(data:image/png;base64,) no-repeat left center / 26rpx;
}

.address_bar .right {
  font-size: 28rpx;
  color: #e4393c;
  display: flex;
  line-height: 90rpx;
}

.right .share {
  padding-left: 20rpx;
}

.right .edit {
  padding-left: 20rpx;
}

/* 商品列表 */

.store {
}

.store .shop_head {
  padding: 0 20rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1rpx solid #e6e6e6;
}

.store .left {
  display: flex;
  align-items: center;
}

.store .left .check {
  width: 40rpx;
  height: 40rpx;
  margin-right: 20rpx;
  border: 2rpx solid #999;
  border-radius: 50%;
}

.store .right {
  display: flex;
}

.store .shop_head .icon {
  width: 40rpx;
  height: 40rpx;
  margin-right: 10rpx;
  background: url(data:image/png;base64,) no-repeat -70rpx -10rpx / 120rpx auto;
}

.store .shop_head .shop_name {
  height: 80rpx;
  font-size: 32rpx;
  color: 333;
  /* padding-left: 40rpx; */
  line-height: 80rpx;
}

.store .coupons {
  float: right;
  font-size: 24rpx;
  color: #e4393c;
}

.item_content {
  padding: 0 20rpx;
}

.item_content .item_head {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding-top: 20rpx;
  /* line-height: 40rpx; */
}

.item_head .left .activity {
  padding: 0 10rpx;
  margin-right: 10rpx;
  color: #fff;
  border-radius: 6rpx;
  background: #e4393c;
  font-size: 20rpx;
  position: relative;
}

.item_head .left .activity::after {
  content: "";
  position: absolute;
  left: 16rpx;
  bottom: -8rpx;
  width: 0;
  height: 0;
  border-top: 8rpx solid #e4393c;
  border-left: 8rpx solid transparent;
  border-right: 8rpx solid transparent;
}

.item_head .left .detail {
  font-size: 24rpx;
  color: #333;
  line-height: 40rpx;
}

.item_head .right {
  font-size: 24rpx;
  color: #999;
  line-height: 40rpx;
  padding-right: 22rpx;
  position: relative;
}

.item_head .right::after {
  content: "";
  width: 12rpx;
  height: 20rpx;
  position: absolute;
  right: 0;
  top: 50%;
  margin-top: -8rpx;
  background: url("data:image/svg+xml,%3Csvg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 12 20%22%3E%3Cpath fill=%22#CCCCCC%22 fill-rule=%22evenodd%22 d=%22M2 20c-.8 0-1.5-.5-1.8-1.2-.3-.8-.2-1.6.4-2.2L7.2 10 .6 3.4c-.8-.8-.8-2 0-2.8.8-.8 2-.8 2.8 0l8 8c.4.4.6 1 .6 1.4 0 .5-.2 1-.6 1.4l-8 8c-.4.4-1 .6-1.4.6z%22/%3E%3C/svg%3E") no-repeat center;
}

.goods {
  padding: 10rpx 0 30rpx 256rpx;
  position: relative;
}

.goods .icon_select {
  position: absolute;
  top: 0;
  left: 0;
  width: 126rpx;
}

.icon_select .check {
  width: 40rpx;
  height: 40rpx;
  margin-right: 20rpx;
  border: 2rpx solid #999;
  border-radius: 50%;
  position: absolute;
  top: 70rpx;
}

.icon_select .checked{
  width: 44rpx;
  height: 44rpx;
  margin-right: 20rpx;
  border-radius: 50%;
  position: absolute;
  top: 70rpx;
  background: url(data:image/png;base64,) no-repeat center /cover
}

.goods .image {
  width: 150rpx;
  height: 150rpx;
  position: absolute;
  top: 10rpx;
  left: 86rpx;
}

.goods .lower_price {
  width: 150rpx;
  position: absolute;
  left: 86rpx;
  top: 160rpx;
  font-size: 20rpx;
  line-height: 20rpx;
  color: #fff;
  background: rgba(228, 57, 60, 0.7);
  padding: 10rpx 0 8rpx;
  text-align: center;
}

.goods .info {
  min-height: 224rpx;
}

.info .name {
  font-size: 28rpx;
  color: #333;
  margin-bottom: 10rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.info .line {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.line .price {
  display: flex;
  align-items: flex-end;
  font-size: 20rpx;
  color: #e4393c;
}

.price .price_now {
  margin-right: 10rpx;
}

.price .price_now .int {
  display:inline-block;
  font-size: 32rpx;
}

.price .price_before {
  color: #999;
}

.info .num {
  display: flex;
  align-items: center;
}

.num .btn {
  width: 60rpx;
  height: 60rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  /* color: #999; */
  border: none;
}
.num .minus_disable{
  color: #eee;
}
.num input.number {
  font-size: 24rpx;
  width: 90rpx;
  height: 60rpx;
  background: #f8f8f8;
  margin: 0 2rpx;
  color: #999;
  text-align: center;
}

.info .cate {
  font-size: 28rpx;
  line-height: 42rpx;
  color: #666;
  margin: 16rpx 0 20rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  padding: 2rpx 50rpx 2rpx 10rpx;
  border: 2rpx solid #e5e5e5;
  border-radius: 4rpx;
  position: relative;
}

.cate .more {
  width: 20rpx;
  height: 12rpx;
  top: 50%;
  right: 12rpx;
  margin-top: -4rpx;
  position: absolute;
  background: url("data:image/svg+xml,%3Csvg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 12 20%22%3E%3Cpath fill=%22#CCCCCC%22 fill-rule=%22evenodd%22 d=%22M2 20c-.8 0-1.5-.5-1.8-1.2-.3-.8-.2-1.6.4-2.2L7.2 10 .6 3.4c-.8-.8-.8-2 0-2.8.8-.8 2-.8 2.8 0l8 8c.4.4.6 1 .6 1.4 0 .5-.2 1-.6 1.4l-8 8c-.4.4-1 .6-1.4.6z%22/%3E%3C/svg%3E") no-repeat center;
  transform: rotate(90deg);
}

.goods .promotion {
  padding-left: 84rpx;
  margin: 10rpx 0 0 -252rpx;
}

.promotion .content {
  padding: 6rpx 0 12rpx;
  background: #fef5f5;
  font-size: 24rpx;
  display: flex;
  align-items: center;
  position: relative;
}

.content::after {
  content: "";
  width: 0;
  height: 0;
  position: absolute;
  left: 66rpx;
  top: -10rpx;
  border-bottom: 10rpx solid #fef5f5;
  border-left: 10rpx solid transparent;
  border-right: 10rpx solid transparent;
}

.content .promo_laber {
  margin: 2rpx 20rpx 0;
  color: #666;
}

.content .right {
  display: flex;
  flex-grow: 1;
  padding-right: 20rpx;
  justify-content: space-between;
  align-items: center;
  color: #999;
}

.content .right .select {
  margin-right: 30rpx;
  position: relative;
}

.content .right .select::after {
  content: "";
  width: 20rpx;
  height: 12rpx;
  position: absolute;
  right: -30rpx;
  top: 50%;
  margin-top: -4rpx;
  background: url("data:image/svg+xml,%3Csvg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 12 20%22%3E%3Cpath fill=%22#CCCCCC%22 fill-rule=%22evenodd%22 d=%22M2 20c-.8 0-1.5-.5-1.8-1.2-.3-.8-.2-1.6.4-2.2L7.2 10 .6 3.4c-.8-.8-.8-2 0-2.8.8-.8 2-.8 2.8 0l8 8c.4.4.6 1 .6 1.4 0 .5-.2 1-.6 1.4l-8 8c-.4.4-1 .6-1.4.6z%22/%3E%3C/svg%3E") no-repeat center;
  transform: rotate(90deg);
}

/* 购物车为空 */

.cart_empty {
  width: 250rpx;
  height: 250rpx;
  margin: 30rpx auto;
  background: transparent url(data:image/gif;base64,) no-repeat center / cover;
}

.notice {
  font-size: 32rpx;
  color: #333;
  margin: 22rpx 0;
  text-align: center;
  line-height: 48rpx;
}

/* 底部 */

.tab {
  z-index: 9999;
  width: 100%;
  height: 100rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
  position: fixed;
  left: 0;
  bottom: 0;
  background: #fff;
}

.tab .select {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: space-around;
}

.select .both {
  font-size: 20rpx;
  line-height: 30rpx;
}

.select .check {
  width: 40rpx;
  height: 40rpx;
  border: 2rpx solid #999;
  border-radius: 50%;
}
.select .checked {
  width: 44rpx;
  height: 44rpx;
  border-radius: 50%;
  background: url('data:image/png;base64,') no-repeat center / cover;
}

.tab .account,.tab .options {
  position: absolute;
  top: 0;
  right: 0;
  display: flex;
  justify-content: center;
  align-items: center;
}



.account .left {
  /* float: left; */
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: flex-end;
}
.options .left{
  width: 220rpx;
  height: 100rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  background: #ff9600;
  color: #fff;
}
.options .del{
  width: 220rpx;
  height: 100rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  background: #e4393c;
  color: #fff;
}
.left .top {
  font-size: 32rpx;
  color: #333;
}

.top .total {
  color: #e4393c;
}

.left .bottom {
  font-size: 20rpx;
  color: #999;
}

.account .pay {
  width: 220rpx;
  height: 100rpx;
  float: right;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-left: 20rpx;
  background: #d7d7d7;
  color: #999;
  font-size: 32rpx;
  text-align: center;
}
.account .pay.active{
  background: #e4393c;
  color: #fff;
}

/* 推荐 */

.recommend {
  background: #f7f7f7;
  padding: 30rpx 0;
}

.recommend .tick {
  /* margin: 30rpx; */
  position: relative;
}

.tick::before {
  content: "";
  position: absolute;
  /* width: 710rpx; *//* padding: 0 20rpx; */
  left: 20rpx;
  right: 20rpx;
  /* z-index: 999; */
  top: 50%;
  height: 2rpx;
  background: #999;
}

.tick .title {
  font-size: 24rpx;
  color: #999;
  text-align: center;
  background: #f7f7f7;
  width: 204rpx;
  margin: 0 auto;
  position: relative;
}

.title::before {
  content: "";
  width: 8rpx;
  height: 8rpx;
  background: #999;
  transform: rotate(45deg);
  position: absolute;
  left: 0;
  top: 50%;
  margin-top: -2rpx;
}

.title::after {
  content: "";
  width: 8rpx;
  height: 8rpx;
  background: #999;
  transform: rotate(45deg);
  position: absolute;
  right: 0;
  top: 50%;
  margin-top: -2rpx;
}

/* 推荐列表 */

.items_list {
  background: #000;
}

.items_list .items_content {
  width: 373rpx;
  /* height: 496rpx; */
  margin: 0 2rpx 2rpx 0;
  padding: 30rpx 30rpx 0;
  background: #fff;
  box-sizing: border-box;
  float: left;
}

.items_list .items_content:nth-of-type(2n) {
  float: right;
  margin-right: -2rpx;
}

.items_content .image {
  width: 314rpx;
  height: 314rpx;
}

.item_content .items_info {
}

.items_info .items_name {
  font-size: 24rpx;
  color: #333;
  margin: 24rpx 0 8rpx;
  line-height: 36rpx;
}

.items_info .price_cart {
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
}

.price_cart .price {
  width: 254rpx;
  font-size: 24rpx;
  line-height: 36rpx;
  color: #333;
}

.price_cart .cart {
  width: 60rpx;
  height: 48rpx;
  margin-top: 20rpx;
  padding: 0 20rpx;
  background: url('data:image/png;base64,') no-repeat center / 40rpx auto;
}
